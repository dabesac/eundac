		<div class="col-md-2">
			<a id="backeditBtn" href="##" class="btn btn-default form-control">Volver</a>
		</div>
	</div>
		<hr><br>
	<div class="row container">
		<form id="form" action="/profile/public/studentnewinterest" method="post">
			<div class="col-sm-12">
			<h4>Intereses</h4>
				<table class="table">
					<tr>
						<td><label>Dedicaci√≥n</label><?php print $this->form->discipline?></td>
					</tr>
					<tr>
						<td><label>Nombre</label><?php print $this->form->title?></td>
